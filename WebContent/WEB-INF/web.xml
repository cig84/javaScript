    <servlet-name>CargarMenu</servlet-name>
    <url-pattern>/CargarMenu</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Persona</servlet-name>
    <servlet-class>beans.Persona</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Persona</servlet-name>
    <url-pattern>/Persona</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>CargaMenuSelect</servlet-name>
    <servlet-class>servicios.CargaMenuSelect</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CargaMenuSelect</servlet-name>
    <url-pattern>/CargaMenuSelect</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>CargaJSON</servlet-name>
    <servlet-class>servicios.CargaJSON</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CargaJSON</servlet-name>
    <url-pattern>/CargaJSON</url-pattern>
  </servlet-mapping>
  <session-config>
    <session-timeout>5</session-timeout>
  </session-config>
</web-app>