  <welcome-file-list>
    <welcome-file>inicio2.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>CargarTrabajadores</servlet-name>
    <servlet-class>servicios.CargarTrabajadores</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CargarTrabajadores</servlet-name>
    <url-pattern>/CargarTrabajadores</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>CargarMenu</servlet-name>
    <servlet-class>servicios.CargarMenu</servlet-class>
  </servlet>
